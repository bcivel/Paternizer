<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>GenerateFile</servlet-name>
        <servlet-class>com.paternizer.servlet.GenerateFile</servlet-class>
        <init-param>
            <param-name>templateURL</param-name>
            <param-value/>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>UploadTemplate</servlet-name>
        <servlet-class>com.paternizer.servlet.UploadTemplate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PublishOnFtp</servlet-name>
        <servlet-class>com.paternizer.servlet.PublishOnFtp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetTemplateList</servlet-name>
        <servlet-class>com.paternizer.servlet.GetTemplateList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GenerateFile</servlet-name>
        <url-pattern>/GenerateFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadTemplate</servlet-name>
        <url-pattern>/UploadTemplate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PublishOnFtp</servlet-name>
        <url-pattern>/PublishOnFtp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetTemplateList</servlet-name>
        <url-pattern>/GetTemplateList</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
